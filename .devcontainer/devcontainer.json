{
  "name": "Graphs and Algorithmic Complexity",
  "image": "mcr.microsoft.com/devcontainers/base:alpine",
  "postCreateCommand": "sudo apk update && sudo apk add just typst gdb zig ghc stack",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cpptools",
        "vadimcn.vscode-lldb",
        "ziglang.vscode-zig",
        "haskell.haskell",
        "myriad-dreamin.tinymist"
      ]
    }
  }
}
